{
  "template": "pinboard*",
  "settings": {
    "index.number_of_shards": 1,
    "index.number_of_replicas": 1,
    "index.codec": "best_compression"
  },
  "mappings": {
    "posts": {
      "properties": {
        "href": {
          "type": "string",
          "index": "not_analyzed"
        },
        "time": {
          "type": "date"
        },
        "lastFetched": {
          "type": "date"
        }
      }
    }
  }
}
